<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.liubiljett.ui.MainPageFragment"
        android:label="@string/title_feed"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.liubiljett.ui.PostFragment"
        android:label="@string/title_post"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.liubiljett.ui.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_notifications" />'
    <fragment
        android:id="@+id/navigation_liked"
        android:name="com.example.liubiljett.ui.LikedFragment"
        android:label="@string/title_liked"
        tools:layout="@layout/fragment_liked" />
</navigation>
